<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>Agendinha</display-name>
<!-- Mapeamento dos Action de Pais -->
    
    <servlet>
        <servlet-name>CadastrarContatoAction</servlet-name>
        <servlet-class>br.agenda.actions.CadastrarContatoAction</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastrarContatoAction</servlet-name>
        <url-pattern>/cadastrar</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ContatoAction</servlet-name>
        <servlet-class>br.agenda.actions.ContatoAction</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>ContatoAction</servlet-name>
        <url-pattern>/ContatoAction</url-pattern>
    </servlet-mapping>
    
    
    
    <servlet>
        <servlet-name>AlterarContatoAction</servlet-name>
        <servlet-class>br.agenda.actions.AlterarContatoAction</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>AlterarContatoAction</servlet-name>
        <url-pattern>/alterar</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ApagarContatoAction</servlet-name>
        <servlet-class>br.agenda.actions.ApagarContatoAction</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>ApagarContatoAction</servlet-name>
        <url-pattern>/remove</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>PesquisarContatoAction</servlet-name>
        <servlet-class>br.agenda.actions.PesquisarContatoAction</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>PesquisarContatoAction</servlet-name>
        <url-pattern>/pesquisar</url-pattern>
    </servlet-mapping>
    
    
    
    <servlet>
        <servlet-name>CarregarContatoAction</servlet-name>
        <servlet-class>br.agenda.actions.CarregarContatoAction</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>CarregarContatoAction</servlet-name>
        <url-pattern>/carregar</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Bairro</servlet-name>
        <servlet-class>br.agenda.actions.BairroAction</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Bairro</servlet-name>
        <url-pattern>/bairro</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Cidade</servlet-name>
        <servlet-class>br.agenda.actions.CidadeAction</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Cidade</servlet-name>
        <url-pattern>/cidade</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Pais</servlet-name>
        <servlet-class>br.agenda.actions.PaisAction</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Pais</servlet-name>
        <url-pattern>/pais</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
		<welcome-file>index.html</welcome-file>
		</welcome-file-list>
</web-app>
